{
  "name": "server",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "set NODE_ENV=dev&&node dist --trace-warnings",
    "dev": "nodemon --exec babel-node src -L",
    "restart": "yarn run rimraf dist && yarn run build && yarn run start",
    "rimraf": "./node_modules/rimraf/bin.js",
    "test": "mocha --recursive --require esm --exit --colors",
    "test-cart": "mocha --recursive --require esm --exit --colors --grep '.Cart'",
    "test-order": "mocha --recursive --require esm --exit --colors --grep '.Order'",
    "test-signup": "mocha --recursive --require esm --exit --colors --grep '.Signup'",
    "seed": "babel-node seed",
    "drop": "babel-node seed/drop.js"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "apollo-server": "^2.19.1",
    "aws-sdk": "^2.865.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.18.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "immer": "^9.0.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mongoose": "^5.11.10",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "apollo-server-testing": "^2.24.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.3.4",
    "chai-graphql": "^4.0.0",
    "cross-env": "^7.0.3",
    "esm": "^3.2.25",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
